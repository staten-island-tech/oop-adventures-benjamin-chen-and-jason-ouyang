class Rock:
    self = True
    def __init__(self, name, atk, health):
        self.name = name
        self.atk = atk
        self.health = health
    
    def attack(self, atk):
        print(f"{self.name} attacked")
        self.atk = atk
    
    def die(self, health):
        if health <= 0:
            self = False
    
rock = Rock("rock", 10, 25)

golem = Rock("golem", 20, 50)
