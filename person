class Person:
    self = True
    def __init__(self, name, atk, health, inventory):
        self.name = name
        self.atk = atk
        self.health = health
        self.inventory = inventory

    def attack(self, atk):
        print(f"{self.name} attacked")
        self.atk == atk

    def heal(self):
        print(f"{self.name} healed")
        self._health += 20
    
    def die(self, health):
        if health <= 0:
            print("you died")
            self = False

You = Person("You", 30, 100)